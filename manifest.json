{
    "manifest_version": 2,
    "name": "Declared Fashion Tech",
    "version": "1.0",
    "permissions": ["contextMenus", "activeTab",   "storage", "downloads"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  